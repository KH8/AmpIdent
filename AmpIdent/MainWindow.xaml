<Window x:Class="AmpIdent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:oxy="clr-namespace:OxyPlot.Wpf;assembly=OxyPlot.Wpf" 
        Title="MainWindow" Height="400" Width="600">
    <Grid>
        <oxy:Plot Model="{Binding Model}" Margin="10,165,10,10" BorderBrush="Black" Background="White" BorderThickness="1"/>
        <Button Content="Load" HorizontalAlignment="Left" Margin="291,10,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox HorizontalAlignment="Left" Height="22" Margin="83,10,0,0" TextWrapping="Wrap" Text="Type file path here" VerticalAlignment="Top" Width="203"/>
        <Label Content="WAV path:" HorizontalAlignment="Left" Height="26" Margin="6,9,0,0" VerticalAlignment="Top" Width="76"/>
        <ListView Name="List" HorizontalAlignment="Left" Height="125" Margin="10,35,10,10" VerticalAlignment="Top" Width="572">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
